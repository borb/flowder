{
    "name": "imjoehaines/flowder",
    "description": "A simple and extensible fixture loader for PHP 5.6+, supporting SQLite and MySQL",
    "keywords": [
        "fixtures",
        "fixture-loading",
        "mysql",
        "sqlite",
        "testing"
    ],
    "homepage": "https://github.com/imjoehaines/flowder",
    "license": "The Unlicense",
    "authors": [
        {
            "name": "Joe Haines",
            "email": "hello@joehaines.co.uk",
            "homepage": "https://www.joehaines.co.uk",
            "role": "Developer"
        }
    ],
    "scripts": {
        "test": "phpunit tests/"
    },
    "config": {
      "platform": {
        "php": "5.6"
      }
    },
    "suggest": {
        "imjoehaines/flowdception": "Support for Flowder in Codeception test suites"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
      "psr-4": {
        "Imjoehaines\\Flowder\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Imjoehaines\\Flowder\\Test\\": "tests/"
      }
    }
}
